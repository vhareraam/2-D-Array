#include<iostream>
using namespace std;
int main()
{
     int n,i,j,c=0;
     cin>>n;
     int a[n][n];
     for(i=0;i<n;i++)
     {
         for(j=0;j<n;j++)
         {
             cin>>a[i][j];
         }
     }
     if(a[0][1]==0 && a[0][2]==0 && a[1][2]==0 && a[2][1]==0 && a[2][2]==0)
     {
         cout<<"Lower triangular matrix";
     }
     else
     {
         cout<<"Not a Lower Triangular Matrix";
     }
     
}
