#include<iostream>
using namespace std;
int main(){
    int i,j,n;
    cin>>n;
    int a[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    cout<<"Transpose matrix is :"<<endl;
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            
            cout<<a[j][i]<<endl;
        }
    }
   
}
