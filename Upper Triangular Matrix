#include<iostream>
using namespace std;
int main()
{
     int n,i,j,c=0;
     cin>>n;
     int a[n][n];
     for(i=0;i<n;i++)
     {
         for(j=0;j<n;j++)
         {
             cin>>a[i][j];
         }
     }
     if( a[n-2][n-n]==0 && a[n-1][n-n]==0 && a[n-1][n-1]==0)
     {
         cout<<"Upper triangular matrix";
     }
     else
     {
         cout<<"Not an Upper Triangular Matrix";
     }
     
}
